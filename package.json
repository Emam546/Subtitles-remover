{
  "name": "subtitles-remover",
  "version": "1.1.7",
  "main": "./out/main/index.js",
  "description": "A subtitles remover",
  "repository": "https://github.com/Emam546/subtitles-remover",
  "author": "Imam Ashour",
  "publish": {
    "owner": "Emam546",
    "provider": "github",
    "releaseType": "release",
    "repo": "subtitles-remover"
  },
  "scripts": {
    "build:next": "next build",
    "export": "npm run build:next&&next export",
    "lint": "next lint -c ./src/.eslintrc.json src",
    "lint:electron": "eslint -c app/.eslintrc.json --ext .ts app/main/",
    "dev": "next dev",
    "test": "jest",
    "build-python": "poetry run pyinstaller --strip --noconfirm --clean --optimize=2 --workpath  ./dist --distpath ./out --name python  python/__main__.py",
    "postinstall": "poetry install",
    "start": "electron-vite preview",
    "build:electron": "electron-vite build",
    "dev-electron": "electron-vite dev",
    "dev:desk": "concurrently -n \"NEXT,ELECTRON\" -c \"yellow,blue\" --kill-others \"next dev\" \"npm run dev-electron\"",
    "build": "npm run build:next&&next export -o ./out/renderer && npm run build:electron",
    "build:win": "npm run build-python&&electron-builder --win -p never",
    "build:inno": "innosetup-compiler setup.iss --verbose",
    "publish": "node release.js",
    "pack": "npm run build&&npm run build:win"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^3.0.0",
    "@ffmpeg-installer/ffmpeg": "^1.1.0",
    "@ffprobe-installer/ffprobe": "^2.1.2",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@joelrbrandt/mp4box.ts": "^1.0.1",
    "@reduxjs/toolkit": "^1.9.3",
    "@tanstack/react-query": "^4.26.1",
    "@tanstack/react-query-devtools": "^4.26.1",
    "axios": "^1.2.4",
    "bcrypt": "^5.1.0",
    "classnames": "^2.5.1",
    "command-line-args": "^5.2.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "easydl": "^1.2.0",
    "electron-serve": "^0.4.0",
    "fluent-ffmpeg": "^2.1.3",
    "fs-extra": "^11.2.0",
    "helmet": "^6.0.1",
    "jet-logger": "^1.2.6",
    "jet-validator": "^1.0.4",
    "js-file-downloader": "^1.1.25",
    "lucide-react": "^0.474.0",
    "mime-types": "^2.1.35",
    "mp4box": "^0.5.3",
    "next": "^13.5.6",
    "node-cache": "^5.1.2",
    "node-fetch": "^2.6.1",
    "react": "18.2.0",
    "react-bootstrap": "^2.10.2",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.49.2",
    "react-player": "^2.16.0",
    "react-range": "^1.8.14",
    "react-redux": "^8.0.7",
    "react-rnd": "^10.4.14",
    "semver": "^7.6.3",
    "throttle": "^1.0.3",
    "unzipper": "^0.12.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.26.7",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bootstrap": "^5.2.6",
    "@types/find": "^0.2.1",
    "@types/fluent-ffmpeg": "^2.1.24",
    "@types/fs-extra": "^11.0.1",
    "@types/jest": "^29.4.0",
    "@types/jsonfile": "^6.1.1",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/supertest": "^2.0.12",
    "@types/throttle": "^1.0.4",
    "@types/unzipper": "^0.10.10",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.7.0",
    "bootstrap": "^5.3.0",
    "concurrently": "^8.2.2",
